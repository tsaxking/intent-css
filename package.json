{
  "name": "intent-css",
  "version": "0.0.1",
  "description": "A styling package written in TypeScript",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && LIB_BUILD=true vite build",
    "build:demo": "vite build",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "preview": "vite preview"
  },
  "keywords": [
    "css",
    "styling",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.10.6",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vite-plugin-dts": "^3.7.0",
    "vitest": "^1.1.0"
  },
  "packageManager": "pnpm@8.0.0"
}
